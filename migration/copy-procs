#!/bin/sh
# Copy the JSON files from svn into our project
# for viewing in Eclipse.
#
SVN=${SVN_REPO}/EMChemicals/G2Artifacts
PROCS=${GIT_REPO}/blt/migration/procs
mkdir -p ${PROCS}/g2/api
mkdir -p ${PROCS}/g2/vistalon
mkdir -p ${PROCS}/ignition/api
mkdir -p ${PROCS}/ignition/vistalon
cp ${SVN}/Source\ code/vist-monitoring/procedures/*.txt ${PROCS}/g2/vistalon
cp ${SVN}/Source\ code/em-diag-toolkit/procedures/OUTPUT-MSG-CORE.txt ${PROCS}/g2/api
