#!/bin/sh
#
# Save the migration files into a zip for distribution.
#
#set -x
SVN=~/Development/ils/svn/EMChemicals/Release/src
VERS=REGRESSION
TMP=${GIT_REPO}/$VERS
# Cleanup
rm -rf $TMP
cd ${GIT_REPO}
mkdir -p $TMP/pylib
cd ${GIT_REPO}/blt/regression
cp -a basic $TMP
cp -a pylib/*.py $TMP/pylib
cd ..
# Now zip and copy to subversion
cd ${GIT_REPO}/$VERS
zip -r blt_regression.zip * 
cp blt_regression.zip $SVN
rm -rf $TMP
